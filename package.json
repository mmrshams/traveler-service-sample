{
    "name": "traveler-travelers",
    "version": "0.8.39",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start:prd": "node build/Server.js",
        "start": "dotenv -e config.development npm run start:dev",
        "start:dev": "nodemon --legacy-watch --config ./nodemon.json src/Server.js --exec 'npm run lint && babel-node'",
        "start:mac": "dotenv -e config.development npm run start:dev:mac",
        "start:dev:mac": "babel-watch src/Server.js",
        "start:debug": "nodemon -L ./playground/build/ --exec 'node --inspect=0.0.0.0 ./playground/build/Server.js'",
        "start:lab": "nodemon --watch tests/component/lab/ --exec node -r dotenv/config tests/component/lab/Server.js dotenv_config_path=config.test-component",
        "start:test:component": "dotenv -e config.test-component npm run start:dev",
        "lint": "eslint src/** --fix",
        "lint:tests": "eslint tests/** --fix",
        "test:component:serial": "dotenv -e config.test-component -- mocha --timeout 200000  tests/component/app/** ",
        "test:component": "npm run lint:tests && npm run test:component:cleanup && npm run test:component:parallel",
        "test:component:cleanup": "dotenv -e config.test-component -- node tests/component/Cleanup.js",
        "test:component:parallel": "dotenv -e config.test-component -- mocha-parallel-tests --timeout 200000 tests/component/app/** tests/component/pubSubSubscribers/**",
        "coverage": "dotenv -e config.test -- nyc --reporter lcov --reporter text mocha --require @babel/register tests/*",
        "doc-publish": "gitbook-comment publish -p ./ -i misc,node_modules,coverage,flow-typed,reports",
        "doc-generate": "gitbook-comment generate -p ./tests",
        "doc-clean-up": "gitbook-comment clean-up -p ./tests",
        "doc": "rm -fr ./docs && docco src/*.js src/**/*.js src/**/**/*.js README.md misc/tutorials/*.md -t ./misc/template/docco.jst -c ./misc/template/docco.css && cp -r ./misc/template/public ./docs",
        "github-pages": "npm run doc && gh-pages-deploy",
        "flow": "flow",
        "build": "rm -fr ./build && npm run build-babel",
        "build-babel": "babel src/ -d build/",
        "build:debug": "rm -fr ./playground/build && cp ./package.json ./playground/ && npm run build-babel:debug",
        "build-babel:debug": "babel --watch src/ -d ./playground/build/ --source-maps inline",
        "release": "standard-version"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@gokiteam/logger": "^1.0.0",
        "@gokiteam/odm": "^1.3.3",
        "@gokiteam/oops": "0.0.2",
        "@gokiteam/pubsub": "^3.1.1",
        "awilix": "^4.2.1",
        "bluebird": "^3.7.0",
        "bodybuilder": "^2.2.17",
        "fs": "0.0.1-security",
        "is-stream": "^2.0.0",
        "joi": "^14.3.1",
        "json-mask": "^1.0.1",
        "koa": "^2.7.0",
        "koa-api-docs": "^1.0.8",
        "koa-body": "^4.1.0",
        "koa-combine-routers": "^4.0.2",
        "koa-router": "^7.4.0",
        "koa2-validation": "^1.0.0",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "pluralize": "^7.0.0",
        "qs": "^6.7.0",
        "redis": "^2.8.0",
        "redlock": "^4.0.0",
        "request": "^2.88.0",
        "rollbar": "^2.14.4",
        "teepee-cloudinary": "^2.0.0",
        "tipi-services": "^5.6.1",
        "uuid": "^3.4.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-transform-flow-strip-types": "^7.3.4",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-flow": "^7.0.0",
        "@babel/register": "^7.4.0",
        "@commitlint/cli": "^8.0.0",
        "@commitlint/config-conventional": "^8.0.0",
        "@stryker-mutator/babel-transpiler": "^2.0.0",
        "@stryker-mutator/core": "^2.0.0",
        "@stryker-mutator/html-reporter": "^2.0.0",
        "@stryker-mutator/javascript-mutator": "^2.0.0",
        "@stryker-mutator/mocha-framework": "^2.0.0",
        "@stryker-mutator/mocha-runner": "^2.0.0",
        "@gokiteam/lab": "0.0.4",
        "@google-cloud/firestore": "^4.1.1",
        "babel-eslint": "^10.0.1",
        "babel-watch": "^7.0.0",
        "chai": "^4.2.0",
        "chai-things": "^0.2.0",
        "chai-http": "^4.3.0",
        "faker": "^4.1.0",
        "coveralls": "^3.0.3",
        "docco": "^0.8.0",
        "dotenv-cli": "^2.0.1",
        "eslint": "^5.15.1",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-chai-expect": "^2.0.1",
        "eslint-plugin-flowtype": "^3.4.2",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-mocha": "^5.3.0",
        "eslint-plugin-node": "^9.0.1",
        "eslint-plugin-promise": "^4.1.1",
        "eslint-plugin-standard": "^4.0.0",
        "fast-check": "^1.15.1",
        "flow-bin": "^0.94.0",
        "gh-pages-deploy": "^0.5.1",
        "gitbook-comment": "^1.0.5",
        "husky": "^2.4.1",
        "mocha": "^6.1.4",
        "mocha-parallel-tests": "^2.2.0",
        "nodemon": "^1.18.10",
        "nyc": "^13.3.0",
        "sinon": "^7.3.2",
        "sinon-chai": "^3.3.0",
        "standard-version": "^6.0.1"
    },
    "gh-pages-deploy": {
        "staticpath": "docs"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ],
        "rules": {
            "subject-case": [
                2,
                "never", [
                    "start-case",
                    "pascal-case"
                ]
            ]
        }
    },
    "standard-version": {
        "message": "chore(release): %s [ci skip]"
    }
}